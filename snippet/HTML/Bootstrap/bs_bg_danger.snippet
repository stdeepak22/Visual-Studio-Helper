<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
  	<Title>bootstrap danger background</Title>
  	<Author>stdeepak22@gmail.com</Author>
  	<Shortcut>bs_bg_danger</Shortcut>
  	<Description>Code snippet to create danger background text</Description>
  	<SnippetTypes>
	  <SnippetType>Expansion</SnippetType>  	  
  	  <SnippetType>SurroundsWith</SnippetType>
  	</SnippetTypes>
  </Header>
  <Snippet>
	<Declarations>
	  <Literal>
	  	<ID>tagName</ID>
	  	<ToolTip>html tag</ToolTip>
	  	<Default>p</Default>
	  </Literal>	
  	</Declarations>  
  	<Code Language="html"><![CDATA[<$tagName$ class="bg-danger">$selected$</$tagName$>$end$]]>
  	</Code>
  </Snippet>
</CodeSnippet>
